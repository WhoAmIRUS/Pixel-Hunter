{
  "env": {
    "browser": true,
    "node": true,
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/prop-types": 0,
    "import/imports-first": 0,

    // to get rid of deperecation notice
    "react/require-extension": 0,
    "import/extensions": 2
  }
}
